<table class="table">
    <thead>
        <tr>
            <th>имя</th>
            <th>номер</th>
            <th>дата</th>
            <th>цвет</th>
            <th>барбер</th>
        </tr>
    </thead>
    <% @results.each do |row| %>
    <tbody>
        <tr>
            <td><%= row['username']%></td>
            <td><%= row['phone']%></td>
            <td><%= row['datestamp']%></td>
            <td><%= row['color']%></td>
            <td><%= row['barber']%></td>
        </tr>
    </tbody>
    <% end %>
</table>